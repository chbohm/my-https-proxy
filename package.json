{
  "name": "my-http-proxy",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ts-node": "node -r ./bin/ts-node.js",
    "ts-node:inspect": "node --inspect=9999 --inspect -r ./bin/ts-node.js",
    "ts-node:inspect:brk": "node --inspect=9999 --inspect-brk -r ./bin/ts-node.js",
    "start": "npm run ts-node src/startProxy.ts",
    "start:dev": "npm run ts-node:inspect src/startProxy.ts",
    "start:dev:brk": "npm run ts-node:inspect:brk src/startProxy.ts"
  },
  "dependencies": {
    "@types/node": "^10.9.3",
    "express": "^4.16.3",
    "http-proxy": "^1.17.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  }
}
